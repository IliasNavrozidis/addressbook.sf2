{% extends "NextAddressBookBundle::base.html.twig" %}

{% block title %}Suppression d'une société - AddressBook{% endblock %}

{% block body %}
<div class="container">
    <h1>Suppression d'une société</h1>
    <p>Etes-vous sûr de vouloir supprimer {{societe.nom}} ?</p>
    <form method="post">
        <input type="submit" class="btn btn-danger" name="confirm" value="Oui">
        <input type="submit" class="btn btn-default" name="confirm" value="Non">
    </form>
</div>
{% endblock %}
